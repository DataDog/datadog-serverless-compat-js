{
  "name": "@datadog/serverless-compat",
  "version": "0.2.0",
  "description": "Datadog Serverless Compatibility Layer to Enable Tracing and Metrics in Node.js",
  "main": "src/index.js",
  "scripts": {
    "lint": "node scripts/check_licenses.js && yarn run eslint . && yarn npm audit"
  },
  "repository": "github.com/DataDog/datadog-serverless-compat-js",
  "keywords": [
    "datadog",
    "serverless"
  ],
  "author": "Datadog",
  "license": "Apache-2.0",
  "packageManager": "yarn@4.5.3",
  "files": [
    "src/**/*",
    "bin/**/*",
    "LICENSE-3rdparty.csv",
    "NOTICE"
  ],
  "publishConfig": {
    "access": "public",
    "executableFiles": [
      "./bin/linux-amd64/datadog-serverless-compat",
      "./bin/windows-amd64/datadog-serverless-compat.exe"
    ]
  },
  "bugs": {
    "url": "https://github.com/DataDog/datadog-serverless-compat-js/issues"
  },
  "homepage": "https://github.com/DataDog/datadog-serverless-compat-js#readme",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0"
  },
  "dependencies": {
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0"
  }
}
